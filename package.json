{
  "name": "change",
  "version": "0.0.1",
  "description": "Zero-dependency, build-free framework",
  "main": "src/index.js",
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "ava": "^5.3.0",
    "browser-env": "^3.3.0",
    "esbuild": "^0.18.0",
    "jsdom": "^22.1.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  },
  "ava": {
    "files": [
      "src/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx"
    ],
    "require": [
      "./src/test-helpers.js"
    ]
  },
  "scripts": {
    "test": "ava",
    "dev": "NODE_ENV=development node esbuild.config.js",
    "build": "NODE_ENV=production node esbuild.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpourismaiel/change.git"
  },
  "keywords": [
    "framework",
    "frontend",
    "javascript"
  ],
  "author": "Mahdi Pourismaiel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mpourismaiel/change/issues"
  },
  "homepage": "https://github.com/mpourismaiel/change#readme"
}
